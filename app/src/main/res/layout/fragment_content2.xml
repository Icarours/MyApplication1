<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <Button
        android:id="@+id/btn_lv_demo1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp" android:layout_marginTop="8dp"
        android:text="lvDemo1" android:textAllCaps="false"
        app:layout_constraintStart_toStartOf="parent" app:layout_constraintTop_toTopOf="parent"/>
    <Button
        android:id="@+id/btn_glide"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        android:text="glide" android:textAllCaps="false"
        app:layout_constraintStart_toEndOf="@+id/btn_lv_demo1"
        app:layout_constraintTop_toTopOf="parent"/>
    <Button
        android:id="@+id/btn_picasso"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp" android:layout_marginTop="8dp"
        android:text="Picasso" android:textAllCaps="false"
        app:layout_constraintStart_toEndOf="@+id/btn_glide"
        app:layout_constraintTop_toTopOf="parent"/>
    <Button
        android:id="@+id/btn_fresco"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp" android:layout_marginStart="8dp"
        android:layout_marginTop="8dp" android:text="fresco"
        android:textAllCaps="false"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/btn_picasso"
        app:layout_constraintTop_toTopOf="parent"/>
    <Button
        android:id="@+id/btn_url_image_loader"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp" android:layout_marginTop="8dp"
        android:text="url_image_loader" android:textAllCaps="false"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_lv_demo1"/>
    <Button
        android:id="@+id/btn_arc"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp" android:text="圆形统计图"
        app:layout_constraintStart_toEndOf="@+id/btn_url_image_loader"
        app:layout_constraintTop_toBottomOf="@+id/btn_glide"/>
    <Button
        android:id="@+id/btn_share"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp" android:text="分享"
        app:layout_constraintStart_toEndOf="@+id/btn_arc"
        app:layout_constraintTop_toBottomOf="@+id/btn_picasso"/>
    <Button
        android:id="@+id/btn_jpush"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp" android:text="极光推送"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_url_image_loader"/>
    <Button
        android:id="@+id/btn_rv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp" android:layout_marginStart="8dp"
        android:layout_marginTop="8dp" android:text="recycleView"
        android:textAllCaps="false" app:layout_constraintStart_toEndOf="@+id/btn_jpush"
        app:layout_constraintTop_toBottomOf="@+id/btn_arc"/>
    <Button
        android:id="@+id/btn_rv2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp" android:layout_marginStart="8dp"
        android:layout_marginTop="8dp" android:text="rv2"
        android:textAllCaps="false" app:layout_constraintStart_toEndOf="@+id/btn_rv"
        app:layout_constraintTop_toBottomOf="@+id/btn_arc"/>
    <Button
        android:id="@+id/btn_rx_easy_http"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp" android:text="RxEasyHttp"
        android:textAllCaps="false" app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_jpush"/>
    <Button
        android:id="@+id/btn_rv3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp" android:layout_marginTop="8dp"
        android:text="rv3" app:layout_constraintStart_toEndOf="@+id/btn_rx_easy_http"
        app:layout_constraintTop_toBottomOf="@+id/btn_rv"/>
    <Button
        android:id="@+id/btn_vp_tab_fm"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp" android:layout_marginStart="8dp"
        android:layout_marginTop="8dp" android:text="vp+tab+fm"
        android:textAllCaps="false" app:layout_constraintStart_toEndOf="@+id/btn_rv3"
        app:layout_constraintTop_toBottomOf="@+id/btn_rv2"/>
    <Button
        android:id="@+id/btn_recorder"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp" android:layout_marginTop="8dp"
        android:text="录音" app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_rx_easy_http"/>
    <Button
        android:id="@+id/btn_recorder2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp" android:layout_marginTop="8dp"
        android:text="录音2" app:layout_constraintStart_toEndOf="@+id/btn_recorder"
        app:layout_constraintTop_toBottomOf="@+id/btn_rv3"/>
    <Button
        android:id="@+id/btn_pop"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp" android:layout_marginStart="8dp"
        android:layout_marginTop="8dp" android:text="弹出式菜单和Toolbar"
        android:textAllCaps="false" app:layout_constraintStart_toEndOf="@+id/btn_recorder2"
        app:layout_constraintTop_toBottomOf="@+id/btn_vp_tab_fm"/>
    <Button
        android:id="@+id/btn_mp_android_chart"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp" android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:text="MPAndroid Chart" android:textAllCaps="false"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_pop"/>
    <Button
        android:id="@+id/btn_drawable"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp" android:layout_marginStart="8dp"
        android:layout_marginTop="8dp" android:text="Drawable"
        android:textAllCaps="false" app:layout_constraintStart_toEndOf="@+id/btn_mp_android_chart"
        app:layout_constraintTop_toBottomOf="@+id/btn_pop"/>
    <Button
        android:id="@+id/btn_get_display_metric"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp" android:layout_marginTop="8dp"
        android:text="屏幕宽高" app:layout_constraintStart_toEndOf="@+id/btn_drawable"
        app:layout_constraintTop_toBottomOf="@+id/btn_pop"/>
    <Button
        android:id="@+id/btn_mobile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp" android:layout_marginTop="8dp"
        android:text="手机参数" app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_mp_android_chart"/>
    <Button
        android:id="@+id/btn_rv_check_box"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp" android:layout_marginStart="8dp"
        android:layout_marginTop="8dp" android:text="Rv_CheckBox"
        android:textAllCaps="false" app:layout_constraintStart_toEndOf="@+id/btn_mobile"
        app:layout_constraintTop_toBottomOf="@+id/btn_mp_android_chart"/>
</android.support.constraint.ConstraintLayout>